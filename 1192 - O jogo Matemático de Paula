#include <iostream>
#include <string.h>
#include <stdio.h>

using namespace std;

int main(){
  int a,c,T,N;
  char b;
  while(scanf("%d", &N) != EOF){
    for (int i=0;i<N;i++){
      cin>>a>>b>>c;
      if(a==c) T=a*c;
      else if(b>=45 && b<=90) T=c-a;
      else if(b>=97 && b<=122) T=a+c;
      cout<<T<<endl;
    }
  }
}
